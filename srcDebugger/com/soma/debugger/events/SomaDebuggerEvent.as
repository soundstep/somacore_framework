tring = "Soma::SomaDebuggerEvent.CLEAR";				public var message:Object;		public var info:String;		public var debuggerName:String;		//------------------------------------		// constructor		//------------------------------------				public function SomaDebuggerEvent(type:String, message:Object = null, info:String = null, debuggerName:String = null, bubbles:Boolean = true, cancelable:Boolean = false) {			this.message = message;			this.info = info;			this.debuggerName = debuggerName;			super(type, bubbles, cancelable);		}				//		// PRIVATE, PROTECTED		//________________________________________________________________________________________________				//		// PUBLIC		//________________________________________________________________________________________________				override public function clone():Event {			return new SomaDebuggerEvent(type, message, info, debuggerName, bubbles, cancelable);		}				override public function toString():String {			return formatToString("SomaDebuggerEvent", "message", "info", "debuggerName", "type", "bubbles", "cancelable", "eventPhase");		}			}}