package com.soma.core.tests.suites.support {	import flash.events.Event;			/**	 * <b>Author:</b> Romuald Quantin - <a href="http://www.soundstep.com/" target="_blank">www.soundstep.com</a><br />	 * <b>Class version:</b> 1.0<br />	 * <b>Actionscript version:</b> 3.0<br />	 * <b>Date:</b> Oct 9, 2010<br />	 * @example	 * <listing version="3.0"></listing>	 */		public class TestEvent extends Event {		//------------------------------------		// private, protected properties		//------------------------------------				//------------------------------------		// public properties		//------------------------------------				public static const TEST:String = "TestEvent.TEST";				public var data:Object;		public var isCloned:Boolean = false;		//------------------------------------		// constructor		//------------------------------------				public function TestEvent(type:String, data:Object = null, bubbles:Boolean = true, cancelable:Boolean = true) {			this.data = data;			super(type, bubbles, cancelable);		}				//		// PRIVATE, PROTECTED		//________________________________________________________________________________________________				//		// PUBLIC		//________________________________________________________________________________________________				override public function clone():Event {
			var event:TestEvent = new TestEvent(type, data, bubbles, cancelable);
			event.isCloned = true;			return event;		}				override public function toString():String {			return formatToString("TestEvent", "data", "isCloned",  "type", "bubbles", "cancelable", "eventPhase");		}			}}